{
  
  "manifest_version":2,
  "name":"Clock.ly",
  "version":"1.0.0",
  "description":"Timezones around the world",
  "author":"Reece Appau",
  "icons":{
    "128":"icon.png"
  },
  
  "browser_action":{
    "default_popup":"popup.html",
    "default_icon":"icon.png",
    "default_title":"Clock.ly"

  },
  
  "permissions":["activeTab"],
  
  "background":{
    "scripts":["main.js"]
  }
  
}